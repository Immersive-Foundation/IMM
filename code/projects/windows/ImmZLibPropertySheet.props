<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <IMM_ZLibIncDir>$(IMMThirdPartyDir)/zlib/include</IMM_ZLibIncDir>
    <IMM_ZLibLibDir>$(IMMThirdPartyDir)/zlib/lib/$(Configuration)</IMM_ZLibLibDir>
  </PropertyGroup>
  <PropertyGroup>
    <IncludePath>$(IMM_ZLibIncDir);$(IncludePath)</IncludePath>
    <LibraryPath>$(IMM_ZLibLibDir);$(LibraryPath)</LibraryPath>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <Link>
      <AdditionalDependencies>zlibstatic.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
</Project>
